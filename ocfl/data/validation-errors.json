{
  "@context": {
    "@version": 1.1,
    "id": "@id",
    "type": "@type",
    "@base": "https://ocfl.io/terms/validation/",
    "ocflv": "https://ocfl.io/terms/validation/",
    "Error": "ocflv:Error",
    "Warning": "ocflv:Warning",
    "Info": "ocflv:Info",
    "codes": {"@id": "ocflv:code", "@container": "@id"},
    "section": "ocfl:section",
    "description": { "@id": "ocflv:description", "@container": "@language" }
  },
  "id": "ocflv:validation_codes",
  "codes": {
    "E001": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#object-conformance-declaration",
      "description": {
         "en": "Missing OCFL Namaste file",
         "zz": "Description in language zz"
      }
    },
    "E002": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#object-conformance-declaration",
      "description": {
         "en": "Namaste file name does not match OCFL Version"
      }
    },
    "E003": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#object-conformance-declaration",
      "description": {
         "en": "OCFL Object NamAsTe file contents do not match file name without leading 0="
      }
    },
    "E004": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#inventory",
      "description": {
         "en": "OCFL Object top-level inventory is missing"
      }
    },
    "E005": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#inventory-digest",
      "description": {
         "en": "OCFL Object top-level inventory is missing sidecar digest file"
      }
    },
    "E100": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#inventory",
      "description": {
         "en": "OCFL Object inventory missing `id` attribute"
      }
    },
    "E101": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#inventory",
      "description": {
         "en": "OCFL Object inventory `id` attribute is empty or badly formed"
      }
    },
    "E102": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#inventory",
      "description": {
         "en": "OCFL Object inventory missing `type` attribute"
      }
    },
    "E103": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#inventory",
      "description": {
         "en": "OCFL Object inventory `type` attribute has wrong value"
      }
    },
    "E104": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#inventory",
      "description": {
         "en": "OCFL Object inventory missing `digestAlgorithm` attribute"
      }
    },
    "E105": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#inventory",
      "params": ["digest_algorithm"],
      "description": {
         "en": "OCFL Object inventory `digestAlgorithm` attribute not an allowed value (got '%s')"
      }
    },
    "E106": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#inventory",
      "description": {
         "en": "OCFL Object inventory missing `head` attribute"
      }
    },
    "E107": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#inventory",
      "description": {
         "en": "OCFL Object inventory missing `manifest` attribute"
      }
    },
    "E108": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#inventory",
      "description": {
         "en": "OCFL Object inventory missing `versions` attribute"
      }
    },
    "E302": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#inventory",
      "params": ["description"],
