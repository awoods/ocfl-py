      }
    },
    "E901": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#object-conformance-declaration",
      "params": ["files"],
      "description": {
         "en": "OCFL Object includes more that one NamAsTe file that looks like an object declaration (got %s)"
      }
    },
    "E912": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#version",
      "description": {
         "en": "OCFL Object state block is not a JSON object"
      }
    },
    "E913": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#manifest",
      "params": ["path"],
      "description": {
         "en": "OCFL Object manifest includes invalid content path %s"
      }
    },
    "E914": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#inventory-structure",
      "params": ["got", "expected"],
      "description": {
         "en": "OCFL Object head attribute doesn't match versions (got %s, expected %s)"
      }
    },
    "E915": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#object-structure",
      "params": ["file"],
      "description": {
         "en": "OCFL Object root contains unexpected file: %s"
      }
    },
    "E916": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#object-structure",
      "params": ["dir"],
      "description": {
         "en": "OCFL Object root contains unexpected directory: %s"
      }
    },
    "E917": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#object-structure",
      "params": ["entry"],
      "description": {
         "en": "OCFL Object root contains unexpected entry that isn't a file or directory: %s"
      }
    },
    "E918": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#object-structure",
      "params": ["entry"],
      "description": {
         "en": "OCFL Object extensions direct contains an unexpected non-directory entry: %s"
      }
    },
    "E987": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/",
      "params": ["path"],
      "description": {
         "en": "No OCFL Object to validate at path %s"
      }
    },
    "W001": {
      "type": "Warning",
      "section": "https://ocfl.io/draft/spec/#version",
      "description": {
         "en": "OCFL Object version descriptions SHOULD include a message key"
      }
    },
    "W002": {
      "type": "Warning",
      "section": "https://ocfl.io/draft/spec/#version",
      "description": {
         "en": "OCFL Object version descriptions SHOULD include a user key"
      }
    },
    "W003": {
      "type": "Warning",
      "section": "https://ocfl.io/draft/spec/#version",
      "description": {
         "en": "OCFL Object version numbers SHOULD NOT be zero-padded"
      }
    },
    "W004": {
      "type": "Warning",
      "section": "https://ocfl.io/draft/spec/#version-directories",
      "params": ["entry"],
      "description": {
         "en": "OCFL Object version directories SHOULD NOT contain any directory except the designated content directory (found %s)"
      }
    },
    "W005": {
      "type": "Warning",
      "section": "https://ocfl.io/draft/spec/#version-directories",
      "description": {
         "en": "OCFL Object version directories SHOULD NOT contain an empty content directory"
      }
    },
    "W006": {
      "type": "Warning",
      "section": "https://ocfl.io/draft/spec/#digests",
      "description": {
         "en": "OCFL Object SHOULD use sha512 but uses sha256 as the DigestAlgorithm"
      }
    },
    "W007": {
      "type": "Warning",
      "section": "https://ocfl.io/draft/spec/#inventory-structure",
      "params": ["id"],
      "description": {
         "en": "OCFL Object id SHOULD be a URI (got %s)"
      }
    },
    "W008": {
      "type": "Warning",
      "section": "https://ocfl.io/draft/spec/#version",
      "description": {
         "en": "OCFL Object version block created date SHOULD include a timezone designator"
      }
    },
    "W009": {
      "type": "Warning",
      "section": "https://ocfl.io/draft/spec/#version",
      "description": {
         "en": "OCFL Object version block created date SHOULD be granular to the seconds level"
      }
    }
  }
}
