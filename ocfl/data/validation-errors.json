      }
    },
    "E303": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#inventory",
      "params": ["extra_files"],
      "description": {
         "en": "OCFL Object includes one or more files that are not mentioned in the inventory (%s)"
      }
    },
    "E305": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#inventory",
      "params": ["digest"],
      "description": {
         "en": "OCFL Object state block includes bad digest (%s)"
      }
    },
    "E306": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#version-directories",
      "params": ["entry"],
      "description": {
         "en": "OCFL Object version diretory includes an illegal file (%s)"
      }
    },
    "E401": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#version",
      "description": {
         "en": "OCFL Object version block does not include a created date or it is malformed"
      }
    },
    "E402": {
      "type": "Error",
      "section": "https://ocfl.io/draft/spec/#version",
      "params": ["description"],
      "description": {
